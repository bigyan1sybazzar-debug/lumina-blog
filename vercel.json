{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.bigyann.com.np" }],
      "destination": "https://bigyann.com.np/$1",
      "statusCode": 301
    },
    {
      "source": "/default",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/blog/fm3g9qgx4JGycFGkc3M3",
      "destination": "/blog/samsung-galaxy-a24-price-in-nepal",
      "statusCode": 301
    }
  ],
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}