{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron/autopilot",
      "schedule": "0 0 * * *"
    }
  ],
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/default",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/index.html",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/blog/:path*",
      "destination": "/:path*",
      "statusCode": 301
    },
    {
      "source": "/blog/fm3g9qgx4JGycFGkc3M3",
      "destination": "/samsung-galaxy-a24-price-in-nepal",
      "statusCode": 301
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/697e5283984a4b3f85621c84e6be1cab.txt",
      "destination": "/697e5283984a4b3f85621c84e6be1cab.txt"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=60, stale-while-revalidate=59"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}